{
  "name": "@danielnarey/componentize",
  "version": "0.2.0",
  "description": "Create an updatable component from any function that generates HTML",
  "source": "index.js",
  "main": "dist/index.js",
  "mjs": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --fix index.js src",
    "release": "npm run lint && npm run build && np",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "npm run lint && npm run build && ava",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielnarey/componentize"
  },
  "keywords": [
    "components",
    "UI",
    "functional",
    "dynamic",
    "responsive",
    "view"
  ],
  "author": "Daniel C. Narey",
  "license": "ISC",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "dequal": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-types": "^1.0.2",
    "jsdom": "^15.1.1",
    "np": "^5.0.3",
    "rollup": "^1.21.2",
    "updtr": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
